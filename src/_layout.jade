-var V = process.env.V || Date.now();
//!= partial("_debug")
doctype
html
  head
    meta(charset='utf-8')
    if environment == "production"
      script.
        var API_URL='';
    else
      script.
        var API_URL='http://local.bubujka.org:3001';
    script.
      var DATA = !{JSON.stringify(public._data)};
      var V = !{V};
      var P = "!{P}";

    script(src=P+'build/vendor.js?v='+V)
    link(rel="stylesheet" href=P+"build/vendor.css?v="+V)

    script(src=P+'javascripts/application.js?v='+V)
    link(rel="stylesheet" href=P+"build/main.css?v="+V)
  body
    != yield
    != partial("_instantclick")
    != partial("_footer")
